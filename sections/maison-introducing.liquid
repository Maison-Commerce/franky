{% endschema %}

<div class="maison_commerce_introducing" style="background-color: {{ section.settings.section_bg_color }}; padding-top: {{ section.settings.padding_top_desktop }}px; padding-bottom: {{ section.settings.padding_bottom_desktop }}px; padding-left: {{ section.settings.padding_left_desktop }}px; padding-right: {{ section.settings.padding_right_desktop }}px;">
  <div class="maison_commerce_introducing__container">
    <div class="maison_commerce_introducing__content">
      <div class="maison_commerce_introducing__image-wrapper">
        {% if section.settings.product_image_desktop != blank %}
          <div class="maison_commerce_introducing__image maison_commerce_introducing__image--desktop">
            {{
              section.settings.product_image_desktop
              | image_url: width: 1200
              | image_tag: loading: 'lazy', alt: section.settings.heading_part1, class: 'maison_commerce_introducing__image-img'
            }}
          </div>
        {% endif %}
        {% if section.settings.product_image_mobile != blank %}
          <div class="maison_commerce_introducing__image maison_commerce_introducing__image--mobile">
            {{
              section.settings.product_image_mobile
              | image_url: width: 800
              | image_tag: loading: 'lazy', alt: section.settings.heading_part1, class: 'maison_commerce_introducing__image-img'
            }}
          </div>
        {% elsif section.settings.product_image_desktop != blank %}
          <div class="maison_commerce_introducing__image maison_commerce_introducing__image--mobile">
            {{
              section.settings.product_image_desktop
              | image_url: width: 800
              | image_tag: loading: 'lazy', alt: section.settings.heading_part1, class: 'maison_commerce_introducing__image-img'
            }}
          </div>
        {% endif %}
        
        {% if section.settings.badge_image != blank %}
          <div class="maison_commerce_introducing__badge">
            {{
              section.settings.badge_image
              | image_url: width: 200
              | image_tag: loading: 'lazy', alt: 'Badge', class: 'maison_commerce_introducing__badge-img'
            }}
          </div>
        {% endif %}
      </div>

      <div class="maison_commerce_introducing__text-content">
        {% if section.settings.heading != blank %}
          <h2 class="maison_commerce_introducing__heading">
            {{ section.settings.heading }}
          </h2>
        {% endif %}

        <div class="maison_commerce_introducing__features">
          {% for block in section.blocks %}
            {% case block.type %}
              {% when 'feature' %}
                <div class="maison_commerce_introducing__feature" {{ block.shopify_attributes }}>
                  {% if block.settings.icon != blank %}
                    <div class="maison_commerce_introducing__feature-icon">
                      {{
                        block.settings.icon
                        | image_url: width: 100
                        | image_tag: loading: 'lazy', alt: block.settings.title, class: 'maison_commerce_introducing__feature-icon-img'
                      }}
                    </div>
                  {% endif %}
                  <div class="maison_commerce_introducing__feature-content">
                    {% if block.settings.title != blank %}
                      <h3 class="maison_commerce_introducing__feature-title">{{ block.settings.title }}</h3>
                    {% endif %}
                    {% if block.settings.description != blank %}
                      <p class="maison_commerce_introducing__feature-description">{{ block.settings.description }}</p>
                    {% endif %}
                  </div>
                </div>
            {% endcase %}
          {% endfor %}
        </div>

        <div class="maison_commerce_introducing__social-proof">
          <div class="maison_commerce_introducing__avatars">
            {% if section.settings.avatar_1 != blank %}
              <div class="maison_commerce_introducing__avatar">
                {{
                  section.settings.avatar_1
                  | image_url: width: 100
                  | image_tag: loading: 'lazy', alt: 'Avatar', class: 'maison_commerce_introducing__avatar-img'
                }}
              </div>
            {% endif %}
            {% if section.settings.avatar_2 != blank %}
              <div class="maison_commerce_introducing__avatar">
                {{
                  section.settings.avatar_2
                  | image_url: width: 100
                  | image_tag: loading: 'lazy', alt: 'Avatar', class: 'maison_commerce_introducing__avatar-img'
                }}
              </div>
            {% endif %}
            {% if section.settings.avatar_3 != blank %}
              <div class="maison_commerce_introducing__avatar">
                {{
                  section.settings.avatar_3
                  | image_url: width: 100
                  | image_tag: loading: 'lazy', alt: 'Avatar', class: 'maison_commerce_introducing__avatar-img'
                }}
              </div>
            {% endif %}
          </div>
          <p class="maison_commerce_introducing__social-proof-text">
            {% if section.settings.social_proof_text != blank %}
              <span>{{ section.settings.social_proof_text }}</span>
            {% endif %}
            {% if section.settings.social_proof_count != blank %}
              <strong>{{ section.settings.social_proof_count }}</strong>
            {% endif %}
            {% if section.settings.social_proof_suffix != blank %}
              <span>{{ section.settings.social_proof_suffix }}</span>
            {% endif %}
          </p>
        </div>

        <div class="maison_commerce_introducing__cta">
          {% if section.settings.button_link != blank and section.settings.button_text != blank %}
            <a href="{{ section.settings.button_link }}" class="maison_commerce_introducing__button">
              <span class="maison_commerce_introducing__button-text">{{ section.settings.button_text }}</span>
              <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none">
                <g clip-path="url(#clip0_8314_1471)">
                    <path d="M23.71 11.2901L18.93 6.51011C18.7894 6.37135 18.6108 6.27736 18.4168 6.23998C18.2228 6.20261 18.0221 6.22353 17.84 6.30011C17.6574 6.37513 17.5011 6.50253 17.3907 6.66625C17.2804 6.82997 17.221 7.02269 17.22 7.22011V10.2201C17.22 10.2864 17.1937 10.35 17.1468 10.3969C17.0999 10.4438 17.0363 10.4701 16.97 10.4701H1.5C1.10218 10.4701 0.720644 10.6281 0.43934 10.9095C0.158035 11.1908 0 11.5723 0 11.9701C0 12.3679 0.158035 12.7495 0.43934 13.0308C0.720644 13.3121 1.10218 13.4701 1.5 13.4701H17C17.0663 13.4701 17.1299 13.4964 17.1768 13.5433C17.2237 13.5902 17.25 13.6538 17.25 13.7201V16.7201C17.25 16.9853 17.3554 17.2397 17.5429 17.4272C17.7304 17.6148 17.9848 17.7201 18.25 17.7201C18.3823 17.7196 18.5132 17.6928 18.6351 17.6413C18.7569 17.5898 18.8674 17.5146 18.96 17.4201L23.74 12.6501C23.8985 12.4696 23.9902 12.2401 24 12.0001C24.0008 11.8685 23.9755 11.738 23.9258 11.6162C23.876 11.4944 23.8027 11.3836 23.71 11.2901Z" fill="currentColor"/>
                </g>
                <defs>
                    <clipPath id="clip0_8314_1471">
                    <rect width="24" height="24" fill="white"/>
                    </clipPath>
                </defs>
              </svg>
            </a>
          {% endif %}

          <div class="maison_commerce_introducing__trust-badges">
            {% for block in section.blocks %}
              {% case block.type %}
                {% when 'trust_badge' %}
                  <div class="maison_commerce_introducing__trust-badge" {{ block.shopify_attributes }}>
                    {% if block.settings.icon != blank %}
                      <div class="maison_commerce_introducing__trust-badge-icon">
                        {{
                          block.settings.icon
                          | image_url: width: 50
                          | image_tag: loading: 'lazy', alt: block.settings.text, class: 'maison_commerce_introducing__trust-badge-icon-img'
                        }}
                      </div>
                    {% endif %}
                    {% if block.settings.text != blank %}
                      <p class="maison_commerce_introducing__trust-badge-text">{{ block.settings.text }}</p>
                    {% endif %}
                  </div>
              {% endcase %}
            {% endfor %}
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

<style>
  .maison_commerce_introducing {
    width: 100%;
    background-color: {{ section.settings.section_bg_color }};
  }

  @media (max-width: 950px) {
    .maison_commerce_introducing {
      padding-top: {{ section.settings.padding_top_mobile }}px !important;
      padding-bottom: {{ section.settings.padding_bottom_mobile }}px !important;
      padding-left: {{ section.settings.padding_left_mobile }}px !important;
      padding-right: {{ section.settings.padding_right_mobile }}px !important;
    }
  }

  .maison_commerce_introducing__container {
    max-width: 1280px;
    margin: 0 auto;
    width: 100%;
  }

  .maison_commerce_introducing__content {
    display: flex;
    gap: 24px;
    align-items: center;
    justify-content: space-between;
    border-radius: 24px;
    overflow: hidden;
  }

  @media (max-width: 950px) {
    .maison_commerce_introducing__content {
      flex-direction: column;
      gap: 20px;
    }
  }

  .maison_commerce_introducing__image-wrapper {
    position: relative;
    width: 100%;
    max-width: 600px;
    aspect-ratio: 1/1;
    height: auto;
    border-radius: 16px;
    overflow: hidden;
  }

  @media (max-width: 950px) {
    .maison_commerce_introducing__image-wrapper {
      width: 100%;
      max-width: 100%;
      aspect-ratio: 1 / 1;
      height: auto;
    }
  }

  .maison_commerce_introducing__image {
    width: 100%;
    height: 100%;
    position: relative;
  }

  .maison_commerce_introducing__image--desktop {
    display: block;
  }

  .maison_commerce_introducing__image--mobile {
    display: none;
  }

  @media (max-width: 950px) {
    .maison_commerce_introducing__image--desktop {
      display: none;
    }

    .maison_commerce_introducing__image--mobile {
      display: block;
    }
  }

  .maison_commerce_introducing__image-img {
    width: 100%;
    height: 100%;
    object-fit: cover;
    display: block;
  }

  .maison_commerce_introducing__badge {
    position: absolute;
    top: 27px;
    right: 27px;
    width: 96px;
    height: 96px;
    z-index: 1;
  }

  @media (max-width: 950px) {
    .maison_commerce_introducing__badge {
      width: 70px;
      height: 70px;
      top: 35px;
      right: 20px;
    }
  }

  .maison_commerce_introducing__badge-img {
    width: 100%;
    height: 100%;
    object-fit: contain;
    display: block;
  }

  .maison_commerce_introducing__text-content {
    width: 100%;
    max-width: 600px;
    display: flex;
    flex-direction: column;
    gap: 32px;
    min-width: 0;
  }

  @media (max-width: 950px) {
    .maison_commerce_introducing__text-content {
      gap: 20px;
      max-width: 100%;
    }
  }

  .maison_commerce_introducing__heading {
    font-family: "Swarsh Daisy", serif;
    font-size: {{ section.settings.heading_font_size_desktop }}px;
    font-weight: 400;
    line-height: 1.3;
    color: {{ section.settings.heading_color }};
    margin: 0;
  }

  .maison_commerce_introducing__heading p {
    margin: 0;
    line-height: inherit;
    color: inherit;
  }

  .maison_commerce_introducing__heading em {
    font-style: italic;
  }

  .maison_commerce_introducing__heading em strong{
    font-weight: 400;
    color: #739D6A;
  }

  @media (max-width: 950px) {
    .maison_commerce_introducing__heading {
      font-size: {{ section.settings.heading_font_size_mobile }}px;
      line-height: 1.2;
      text-align: left;
    }
  }

  .maison_commerce_introducing__features {
    display: flex;
    flex-direction: column;
    gap: 24px;
  }

  @media (max-width: 950px) {
    .maison_commerce_introducing__features {
      gap: 18px;
    }
  }

  .maison_commerce_introducing__feature {
    display: flex;
    gap: 18px;
    align-items: center;
  }

  @media (max-width: 950px) {
    .maison_commerce_introducing__feature {
      gap: 12px;
    }
  }

  .maison_commerce_introducing__feature-icon {
    flex-shrink: 0;
    width: 52px;
    height: 52px;
    background: white;
    border: 1px solid rgba(0, 0, 0, 0.05);
    border-radius: 9.455px;
    display: flex;
    align-items: center;
    justify-content: center;
    overflow: hidden;
  }

  @media (max-width: 950px) {
    .maison_commerce_introducing__feature-icon {
      width: 44px;
      height: 44px;
      border-radius: 8px;
    }
  }

  .maison_commerce_introducing__feature-icon-img {
    width: 20.8px;
    height: 20.8px;
    object-fit: contain;
  }

  @media (max-width: 950px) {
    .maison_commerce_introducing__feature-icon-img {
      width: 20px;
      height: 20px;
    }
  }

  .maison_commerce_introducing__feature-content {
    flex: 1;
    display: flex;
    flex-direction: column;
    gap: 4px;
    min-width: 0;
  }

  .maison_commerce_introducing__feature-title {
    font-family: "Neue Haas Grotesk Display Pro", sans-serif;
    font-size: {{ section.settings.feature_title_font_size_desktop }}px;
    font-weight: 600;
    line-height: 1.5;
    color: {{ section.settings.feature_text_color }};
    margin: 0;
  }

  @media (max-width: 950px) {
    .maison_commerce_introducing__feature-title {
      font-size: {{ section.settings.feature_title_font_size_mobile }}px;
    }
  }

  .maison_commerce_introducing__feature-description {
    font-family: "Neue Haas Grotesk Display Pro", sans-serif;
    font-size: {{ section.settings.feature_description_font_size_desktop }}px;
    font-weight: 500;
    line-height: 1.5;
    color: {{ section.settings.feature_text_color }};
    margin: 0;
  }

  @media (max-width: 950px) {
    .maison_commerce_introducing__feature-description {
      font-size: {{ section.settings.feature_description_font_size_mobile }}px;
    }
  }

  .maison_commerce_introducing__social-proof {
    display: flex;
    gap: 12px;
    align-items: center;
  }

  .maison_commerce_introducing__avatars {
    display: flex;
    align-items: center;
    flex-shrink: 0;
  }

  .maison_commerce_introducing__avatar {
    width: 32px;
    height: 32px;
    border-radius: 50%;
    overflow: hidden;
    margin-right: -6px;
    border: 2px solid white;
    flex-shrink: 0;
