      "id": "button_link",
      "label": "Button Link"
    },
    {
      "type": "color",
      "id": "button_bg_color",
      "label": "Button Background Color",
      "default": "#B9FCAB"
    },
    {
      "type": "color",
      "id": "button_text_color",
      "label": "Button Text Color",
      "default": "#2C2C2C"
    },
    {
      "type": "range",
      "id": "button_font_size_desktop",
      "min": 14,
      "max": 24,
      "step": 1,
      "unit": "px",
      "label": "Button Font Size (Desktop)",
      "default": 18
    },
    {
      "type": "range",
      "id": "button_font_size_mobile",
      "min": 14,
      "max": 20,
      "step": 1,
      "unit": "px",
      "label": "Button Font Size (Mobile)",
      "default": 16
    },
    {
      "type": "range",
      "id": "button_padding_top",
      "min": 12,
      "max": 24,
      "step": 1,
      "unit": "px",
      "label": "Button Padding Top",
      "default": 16
    },
    {
      "type": "range",
      "id": "button_padding_bottom",
      "min": 12,
      "max": 24,
      "step": 1,
      "unit": "px",
      "label": "Button Padding Bottom",
      "default": 16
    },
    {
      "type": "range",
      "id": "button_padding_left",
      "min": 24,
      "max": 48,
      "step": 2,
      "unit": "px",
      "label": "Button Padding Left",
      "default": 32
    },
    {
      "type": "range",
      "id": "button_padding_right",
      "min": 24,
      "max": 48,
      "step": 2,
      "unit": "px",
      "label": "Button Padding Right",
      "default": 32
    },
    {
      "type": "range",
      "id": "button_border_radius",
      "min": 0,
      "max": 50,
      "step": 2,
      "unit": "px",
      "label": "Button Border Radius",
      "default": 8
    },
    {
      "type": "header",
      "content": "Trust Badge Settings"
    },
    {
      "type": "range",
      "id": "trust_badge_font_size_desktop",
      "min": 12,
      "max": 20,
      "step": 1,
      "unit": "px",
      "label": "Trust Badge Font Size (Desktop)",
      "default": 14
    },
    {
      "type": "range",
      "id": "trust_badge_font_size_mobile",
      "min": 12,
      "max": 18,
      "step": 1,
      "unit": "px",
      "label": "Trust Badge Font Size (Mobile)",
      "default": 14
    },
    {
      "type": "color",
      "id": "trust_badge_color",
      "label": "Trust Badge Text Color",
      "default": "#2C2C2C"
    },
    {
      "type": "range",
      "id": "trust_badge_gap",
      "min": 16,
      "max": 48,
      "step": 2,
      "unit": "px",
      "label": "Trust Badge Gap",
      "default": 32
    },
    {
      "type": "range",
      "id": "content_gap_desktop",
      "min": 20,
      "max": 80,
      "step": 2,
      "unit": "px",
      "label": "Content Gap (Desktop)",
      "default": 60
    },
    {
      "type": "range",
      "id": "content_gap_mobile",
      "min": 20,
      "max": 60,
      "step": 2,
      "unit": "px",
      "label": "Content Gap (Mobile)",
      "default": 40
    }
  ],
  "blocks": [
    {
      "type": "benefit",
      "name": "Benefit",
      "settings": [
        {
          "type": "richtext",
          "id": "text",
          "label": "Benefit Text",
          "default": "<p>You've got sh*t to do - and <strong>need to actually lock in</strong></p>"
        }
      ]
    },
    {
      "type": "trust_badge",
      "name": "Trust Badge",
      "settings": [
        {
          "type": "image_picker",
          "id": "icon",
          "label": "Icon"
        },
        {
          "type": "text",
          "id": "text",
          "label": "Text",
          "default": "GMP-Certified Facility"
        }
      ]
    }
  ],
  "presets": [
    {
      "name": "Maison Introducing V2",
      "blocks": [
        {
          "type": "benefit"
        },
        {
          "type": "benefit"
        },
        {
          "type": "benefit"
        },
        {
          "type": "benefit"
        },
        {
          "type": "benefit"
        },
        {
          "type": "trust_badge"
        },
        {
          "type": "trust_badge"
        },
        {
          "type": "trust_badge"
        }
      ]
    }
  ]
}
{% endschema %}

<div class="maison_commerce_introducing_v2" style="background-color: {{ section.settings.section_bg_color }}; padding-top: {{ section.settings.padding_top_desktop }}px; padding-bottom: {{ section.settings.padding_bottom_desktop }}px; padding-left: {{ section.settings.padding_left_desktop }}px; padding-right: {{ section.settings.padding_right_desktop }}px;">
  <div class="maison_commerce_introducing_v2__container">
    <div class="maison_commerce_introducing_v2__content">
      <div class="maison_commerce_introducing_v2__text-content">
        {% if section.settings.heading != blank %}
          <h2 class="maison_commerce_introducing_v2__heading">
            {{ section.settings.heading }}
          </h2>
        {% endif %}

        <div class="maison_commerce_introducing_v2__benefits">
          {% for block in section.blocks %}
            {% case block.type %}
              {% when 'benefit' %}
                <div class="maison_commerce_introducing_v2__benefit" {{ block.shopify_attributes }}>
                  <div class="maison_commerce_introducing_v2__benefit-icon">
                    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" viewBox="0 0 16 16" fill="none">
                        <g clip-path="url(#clip0_8125_28085{{ forloop.index }})">
                            <rect width="16" height="16" fill="#F9F9F9"/>
                            <path d="M7.99999 0.166504C3.67376 0.166504 0.166656 3.6736 0.166656 7.99984C0.166656 12.3261 3.67376 15.8332 7.99999 15.8332C12.3262 15.8332 15.8333 12.3261 15.8333 7.99984C15.8333 3.6736 12.3262 0.166504 7.99999 0.166504Z" fill="#2E432F"/>
                            <path d="M11.5558 5.26807L6.66695 10.157L4.44473 7.93473" stroke="#B9FCAB" stroke-width="1.125" stroke-linecap="round" stroke-linejoin="round"/>
                        </g>
                        <defs>
                            <clipPath id="clip0_8125_28085{{ forloop.index }}">
                            <rect width="16" height="16" fill="white"/>
                            </clipPath>
                        </defs>
                    </svg>
                  </div>
                  <div class="maison_commerce_introducing_v2__benefit-text">
                    {{ block.settings.text }}
                  </div>
                </div>
            {% endcase %}
          {% endfor %}
        </div>

        <div class="maison_commerce_introducing_v2__bottom_content">
             <div class="maison_commerce_introducing_v2__social-proof">
                <div class="maison_commerce_introducing_v2__avatars">
                    {% if section.settings.avatar_1 != blank %}
                    <div class="maison_commerce_introducing_v2__avatar">
                        {{
                        section.settings.avatar_1
                        | image_url: width: 100
                        | image_tag: loading: 'lazy', alt: 'Avatar', class: 'maison_commerce_introducing_v2__avatar-img'
                        }}
                    </div>
                    {% endif %}
                    {% if section.settings.avatar_2 != blank %}
                    <div class="maison_commerce_introducing_v2__avatar">
                        {{
                        section.settings.avatar_2
                        | image_url: width: 100
                        | image_tag: loading: 'lazy', alt: 'Avatar', class: 'maison_commerce_introducing_v2__avatar-img'
                        }}
                    </div>
                    {% endif %}
                    {% if section.settings.avatar_3 != blank %}
                    <div class="maison_commerce_introducing_v2__avatar">
                        {{
                        section.settings.avatar_3
                        | image_url: width: 100
                        | image_tag: loading: 'lazy', alt: 'Avatar', class: 'maison_commerce_introducing_v2__avatar-img'
                        }}
                    </div>
                    {% endif %}
                    {% if section.settings.avatar_4 != blank %}
                    <div class="maison_commerce_introducing_v2__avatar">
                        {{
                        section.settings.avatar_4
                        | image_url: width: 100
                        | image_tag: loading: 'lazy', alt: 'Avatar', class: 'maison_commerce_introducing_v2__avatar-img'
                        }}
                    </div>
                    {% endif %}
                </div>
                <p class="maison_commerce_introducing_v2__social-proof-text">
                    {% if section.settings.social_proof_text != blank %}
                    <span>{{ section.settings.social_proof_text }}</span>
                    {% endif %}
                    {% if section.settings.social_proof_count != blank %}
                    <strong>{{ section.settings.social_proof_count }}</strong>
                    {% endif %}
                    {% if section.settings.social_proof_suffix != blank %}
                    <span>{{ section.settings.social_proof_suffix }}</span>
                    {% endif %}
                </p>
                </div>

                <div class="maison_commerce_introducing_v2__cta">
                {% if section.settings.button_link != blank and section.settings.button_text != blank %}
                    <a href="{{ section.settings.button_link }}" class="maison_commerce_introducing_v2__button">
                    <span class="maison_commerce_introducing_v2__button-text">{{ section.settings.button_text }}</span>
                    <svg class="maison_commerce_introducing_v2__button-arrow" xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none">
                        <path d="M5 12H19M19 12L12 5M19 12L12 19" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
                    </svg>
                    </a>
                {% endif %}

                <div class="maison_commerce_introducing_v2__trust-badges">
                    {% for block in section.blocks %}
                    {% case block.type %}
                        {% when 'trust_badge' %}
                        <div class="maison_commerce_introducing_v2__trust-badge" {{ block.shopify_attributes }}>
                            {% if block.settings.icon != blank %}
                            <div class="maison_commerce_introducing_v2__trust-badge-icon">
                                {{
                                block.settings.icon
                                | image_url: width: 50
                                | image_tag: loading: 'lazy', alt: block.settings.text, class: 'maison_commerce_introducing_v2__trust-badge-icon-img'
                                }}
                            </div>
                            {% endif %}
                            {% if block.settings.text != blank %}
                            <p class="maison_commerce_introducing_v2__trust-badge-text">{{ block.settings.text }}</p>
                            {% endif %}
                        </div>
                    {% endcase %}
                    {% endfor %}
                </div>
                </div>
        </div>
      </div>

      <div class="maison_commerce_introducing_v2__image-content">
        {% if section.settings.product_image_desktop != blank %}
          <div class="maison_commerce_introducing_v2__image-wrapper maison_commerce_introducing_v2__image-wrapper--desktop">
            {{
              section.settings.product_image_desktop
              | image_url: width: 800
              | image_tag: loading: 'lazy', alt: 'Product', class: 'maison_commerce_introducing_v2__image'
            }}
          </div>
        {% endif %}
        {% if section.settings.product_image_mobile != blank %}
          <div class="maison_commerce_introducing_v2__image-wrapper maison_commerce_introducing_v2__image-wrapper--mobile">
            {{
              section.settings.product_image_mobile
              | image_url: width: 600
              | image_tag: loading: 'lazy', alt: 'Product', class: 'maison_commerce_introducing_v2__image'
            }}
          </div>
        {% endif %}
      </div>
    </div>
  </div>
</div>

<style>
  #shopify-section-{{ section.id }}{
    padding: 20px;
  }
  .maison_commerce_introducing_v2 {
    width: 100%;
    border-radius: 24px;
    background-color: {{ section.settings.section_bg_color }};
  }

  @media (max-width: 950px) {
    .maison_commerce_introducing_v2 {
      padding-top: {{ section.settings.padding_top_mobile }}px !important;
      padding-bottom: {{ section.settings.padding_bottom_mobile }}px !important;
      padding-left: {{ section.settings.padding_left_mobile }}px !important;
      padding-right: {{ section.settings.padding_right_mobile }}px !important;
    }

    #shopify-section-{{ section.id }}{
        padding: 10px;
    }
  }

  .maison_commerce_introducing_v2__container {
    max-width: 1280px;
    margin: 0 auto;
    width: 100%;
  }

  .maison_commerce_introducing_v2__content {
    display: flex;
    justify-content: space-between;
    gap: {{ section.settings.content_gap_desktop }}px;
    align-items: center;
  }

  @media (max-width: 950px) {
    .maison_commerce_introducing_v2__content {
      flex-direction: column;
      gap: {{ section.settings.content_gap_mobile }}px;
    }
  }

  .maison_commerce_introducing_v2__text-content {
    display: flex;
    flex-direction: column;
    gap: 32px;
    width: 100%;
    max-width: 600px;
  }

  @media (max-width: 950px) {
    .maison_commerce_introducing_v2__text-content {
      gap: 20px;
      max-width: 100%;
    }
  }

  .maison_commerce_introducing_v2__heading {
    font-family: "Swarsh Daisy";
    font-size: {{ section.settings.heading_font_size_desktop }}px;
    font-weight: 400;
    line-height: {{ section.settings.heading_line_height_desktop }};
    color: {{ section.settings.heading_color }};
    margin: 0;
  }

  .maison_commerce_introducing_v2__heading p{
    margin: 0;
  }

  .maison_commerce_introducing_v2__heading em {
    font-style: italic;
    color: {{ section.settings.heading_accent_color }};
  }

  @media (max-width: 950px) {
    .maison_commerce_introducing_v2__heading {
      font-size: {{ section.settings.heading_font_size_mobile }}px;
      line-height: {{ section.settings.heading_line_height_mobile }};
    }
  }

  .maison_commerce_introducing_v2__benefits {
    display: flex;
    flex-direction: column;
    gap: 4px;
  }

  .maison_commerce_introducing_v2__bottom_content{
    display: flex;
    gap: 18px;
    flex-direction: column;
  }

  @media (max-width: 950px) {
    .maison_commerce_introducing_v2__bottom_content{
      margin-top: 12px;
      gap: 16px;
    }
  }

  .maison_commerce_introducing_v2__benefit {
    display: flex;
    gap: 8px;
    align-items: center;
  }

  .maison_commerce_introducing_v2__benefit-icon {
    flex-shrink: 0;
    width: 16px;
    height: 16px;
  }

  .maison_commerce_introducing_v2__benefit-text {
    font-family: "Neue Haas Grotesk Display Pro", sans-serif;
    font-size: 16px;
    font-weight: 400;
    line-height: 1.5;
    color: #2C2C2C;
    margin: 0;
  }

  .maison_commerce_introducing_v2__benefit-text p{
    margin: 0;
  }

  .maison_commerce_introducing_v2__benefit-text strong {
    font-weight: 600;
  }

  @media (max-width: 950px) {
    .maison_commerce_introducing_v2__benefit-text {
      font-size: 14px;
    }
  }

  .maison_commerce_introducing_v2__social-proof {
    display: flex;
    align-items: center;
    gap: 12px;
  }

  .maison_commerce_introducing_v2__avatars {
    display: flex;
    align-items: center;
    margin-right: -8px;
  }

  .maison_commerce_introducing_v2__avatar {
    width: 32px;
    height: 32px;
    border-radius: 50%;
    overflow: hidden;
    border: 2px solid #FFFEF3;
    margin-left: -8px;
    flex-shrink: 0;
  }

  .maison_commerce_introducing_v2__avatar:first-child {
    margin-left: 0;
  }

  .maison_commerce_introducing_v2__avatar-img {
    width: 100%;
    height: 100%;
    object-fit: cover;
    display: block;
  }

  .maison_commerce_introducing_v2__social-proof-text {
    font-family: "Geist", sans-serif;
    font-size: {{ section.settings.social_proof_font_size_desktop }}px;
    font-weight: 400;
    line-height: 1.5;
    color: {{ section.settings.social_proof_color }};
    margin: 0;
  }

  .maison_commerce_introducing_v2__social-proof-text strong {
    font-weight: 600;
  }

  @media (max-width: 950px) {
    .maison_commerce_introducing_v2__social-proof-text {
      font-size: {{ section.settings.social_proof_font_size_mobile }}px;
    }
  }

  .maison_commerce_introducing_v2__cta {
    display: flex;
    flex-direction: column;
    gap: 16px;
    align-items: flex-start;
  }

  @media (max-width: 950px) {
    .maison_commerce_introducing_v2__cta {
      gap: 8px;
    }
  }

  .maison_commerce_introducing_v2__button {
    width: 100%;
    display: inline-flex;
    align-items: center;
    justify-content: center;
    gap: 8px;
    background-color: {{ section.settings.button_bg_color }};
    color: {{ section.settings.button_text_color }};
    text-decoration: none;
    padding: {{ section.settings.button_padding_top }}px {{ section.settings.button_padding_right }}px {{ section.settings.button_padding_bottom }}px {{ section.settings.button_padding_left }}px;
    border-radius: {{ section.settings.button_border_radius }}px;
