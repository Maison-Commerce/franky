{% schema %}
{
  "name": "Maison Banner",
  "settings": [
    {
      "type": "text",
      "id": "section_heading_part1",
      "label": "Section Heading Part 1",
      "default": "Mornings That Kickstart"
    },
    {
      "type": "text",
      "id": "section_heading_part2",
      "label": "Section Heading Part 2 (Italic)",
      "default": "Your Best Life"
    },
    {
      "type": "richtext",
      "id": "section_description",
      "label": "Description",
      "default": "<p>Simply select any 20-pack, 30-pack, or bundle of your choice and add it to your cart. After that, include the Glow Up item, and you'll see the discount applied automatically at checkout!</p>"
    },
    {
      "type": "text",
      "id": "rating_text",
      "label": "Rating Text",
      "default": "Rated 4.8/5 based on 8,450+ reviews"
    },
    {
      "type": "text",
      "id": "benefit_1",
      "label": "Benefit 1",
      "default": "Sustained energy"
    },
    {
      "type": "text",
      "id": "benefit_2",
      "label": "Benefit 2",
      "default": "Complete nutrition"
    },
    {
      "type": "text",
      "id": "benefit_3",
      "label": "Benefit 3",
      "default": "Zero compromise"
    },
    {
      "type": "text",
      "id": "button_text",
      "label": "Button Text",
      "default": "Explore Our Solutions"
    },
    {
      "type": "url",
      "id": "button_link",
      "label": "Button Link"
    },
    {
      "type": "image_picker",
      "id": "product_image_desktop",
      "label": "Product Image (Desktop)"
    },
    {
      "type": "image_picker",
      "id": "product_image_mobile",
      "label": "Product Image (Mobile)"
    },
    {
      "type": "range",
      "id": "padding_top_desktop",
      "min": 0,
      "max": 200,
      "step": 5,
      "unit": "px",
      "label": "Padding Top (Desktop)",
      "default": 20
    },
    {
      "type": "range",
      "id": "padding_bottom_desktop",
      "min": 0,
      "max": 200,
      "step": 5,
      "unit": "px",
      "label": "Padding Bottom (Desktop)",
      "default": 0
    },
    {
      "type": "range",
      "id": "padding_left_desktop",
      "min": 0,
      "max": 200,
      "step": 5,
      "unit": "px",
      "label": "Padding Left (Desktop)",
      "default": 20
    },
    {
      "type": "range",
      "id": "padding_right_desktop",
      "min": 0,
      "max": 200,
      "step": 5,
      "unit": "px",
      "label": "Padding Right (Desktop)",
      "default": 20
    },
    {
      "type": "range",
      "id": "padding_top_mobile",
      "min": 0,
      "max": 100,
      "step": 5,
      "unit": "px",
      "label": "Padding Top (Mobile)",
      "default": 20
    },
    {
      "type": "range",
      "id": "padding_bottom_mobile",
      "min": 0,
      "max": 100,
      "step": 5,
      "unit": "px",
      "label": "Padding Bottom (Mobile)",
      "default": 0
    },
    {
      "type": "range",
      "id": "padding_left_mobile",
      "min": 0,
      "max": 100,
      "step": 5,
      "unit": "px",
      "label": "Padding Left (Mobile)",
      "default": 20
    },
    {
      "type": "range",
      "id": "padding_right_mobile",
      "min": 0,
      "max": 100,
      "step": 5,
      "unit": "px",
      "label": "Padding Right (Mobile)",
      "default": 20
    },
    {
      "type": "color",
      "id": "content_bg_color",
      "label": "Content Background Color",
      "default": "#F8FFF6"
    },
    {
      "type": "color",
      "id": "image_bg_color",
      "label": "Image Background Color",
      "default": "#FFF5F0"
    },
    {
      "type": "range",
      "id": "heading_font_size_desktop",
      "min": 24,
      "max": 72,
      "step": 2,
      "unit": "px",
      "label": "Heading Font Size (Desktop)",
      "default": 48
    },
    {
      "type": "range",
      "id": "heading_font_size_mobile",
      "min": 24,
      "max": 48,
      "step": 2,
      "unit": "px",
      "label": "Heading Font Size (Mobile)",
      "default": 32
    },
    {
      "type": "color",
      "id": "heading_color",
      "label": "Heading Color",
      "default": "#2C2926"
    },
    {
      "type": "range",
      "id": "body_font_size_desktop",
      "min": 14,
      "max": 24,
      "step": 1,
      "unit": "px",
      "label": "Body Font Size (Desktop)",
      "default": 16
    },
    {
      "type": "range",
      "id": "body_font_size_mobile",
      "min": 14,
      "max": 20,
      "step": 1,
      "unit": "px",
      "label": "Body Font Size (Mobile)",
      "default": 16
    },
    {
      "type": "color",
      "id": "button_bg_color",
      "label": "Button Background Color",
      "default": "#2E432F"
    },
    {
      "type": "color",
      "id": "button_text_color",
      "label": "Button Text Color",
      "default": "#FFFFFF"
    },
    {
      "type": "color",
      "id": "button_hover_bg_color",
      "label": "Button Hover Background Color",
      "default": "#1E2F1F"
    },
    {
      "type": "color",
      "id": "checkmark_color",
      "label": "Checkmark Color",
      "default": "#2E432F"
    }
  ],
  "presets": [
    {
      "name": "Maison Banner"
    }
  ]
}
{% endschema %}

