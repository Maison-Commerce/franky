              <div class="maison-banner-section-content-main-rating-text">
                <p>
                  {% assign rating_text = section.settings.rating_text %}
                  {% if rating_text contains '4.8/5' %}
                    {% assign parts = rating_text | split: '4.8/5' %}
                    {{ parts[0] }}<strong>4.8/5</strong>
                    {% if parts[1] contains '8,450+' %}
                      {% assign parts2 = parts[1] | split: '8,450+' %}
                      {{ parts2[0] }}<strong>8,450+</strong>{{ parts2[1] }}
                    {% else %}
                      {{ parts[1] }}
                    {% endif %}
                  {% else %}
                    {{ rating_text }}
                  {% endif %}
                </p>
              </div>
            </div>
          {% endif %}

          {% if section.settings.section_heading_part1 != blank or section.settings.section_heading_part2 != blank %}
            <div class="maison-banner-section-content-main-text-heading">
              <p>
                {% if section.settings.section_heading_part1 != blank %}
                  {{ section.settings.section_heading_part1 }}
                {% endif %}
                {% if section.settings.section_heading_part2 != blank %}
                  <em>{{ section.settings.section_heading_part2 }}</em>
                {% endif %}
              </p>
            </div>
          {% endif %}

          {% if section.settings.section_description != blank %}
            <div class="maison-banner-section-content-main-text-description">
              <p>{{ section.settings.section_description | remove: '<p>' | remove: '</p>' }}</p>
            </div>
          {% endif %}
        </div>
